{
  "name": "@jsonpathx/jsonpathx",
  "version": "0.1.6",
  "private": false,
  "description": "Modern, high-performance JSONPath library with RFC 9535 compliance and jsonpath-plus compatibility.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/jsonpathx.umd.min.js",
  "jsdelivr": "./dist/jsonpathx.umd.min.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./umd": {
      "default": "./dist/jsonpathx.umd.js"
    },
    "./umd.min": {
      "default": "./dist/jsonpathx.umd.min.js"
    }
  },
  "scripts": {
    "build:tsc": "tsc -p tsconfig.json",
    "build:umd": "node scripts/build-umd.mjs",
    "build": "npm run build:tsc && npm run build:umd",
    "prepare": "npm run build",
    "test": "node --test --import tsx tests/**/*.test.ts",
    "size": "node scripts/size.mjs"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonpathx/jsonpathx.git"
  },
  "keywords": [
    "jsonpath",
    "json",
    "query",
    "typescript",
    "rfc9535",
    "jsonpath-plus",
    "json-query",
    "json-selector",
    "json-path",
    "json-pointer",
    "performance"
  ],
  "author": "Afzal Ahmad",
  "license": "MIT",
  "homepage": "https://jsonpathx.github.io/jsonpathx/",
  "bugs": {
    "url": "https://github.com/jsonpathx/jsonpathx/issues"
  },
  "devDependencies": {
    "@types/node": "^20.19.32",
    "esbuild": "^0.21.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
